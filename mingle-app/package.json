{
  "name": "mingle-app",
  "version": "0.1.0",
  "license": "GPL-3.0-only",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "prisma generate && next build",
    "rn:install": "pnpm --dir rn install",
    "rn:pods": "cd rn/ios && (bundle exec pod install || pod install)",
    "rn:start": "node scripts/run-with-env-local.mjs pnpm --dir rn start",
    "rn:ios": "node scripts/run-with-env-local.mjs pnpm --dir rn ios",
    "rn:android": "node scripts/run-with-env-local.mjs pnpm --dir rn android",
    "start": "next start",
    "test": "vitest run",
    "test:unit": "vitest run --exclude \"src/integration/live/**/*.test.ts\"",
    "test:watch": "vitest",
    "test:live": "vitest run src/integration/live",
    "lint": "eslint",
    "db:generate": "node scripts/run-with-env-local.mjs prisma generate",
    "db:migrate:create": "node scripts/run-with-env-local.mjs prisma migrate dev --create-only",
    "db:migrate:dev": "node scripts/run-with-env-local.mjs prisma migrate dev",
    "db:reset:local:psql": "psql -v ON_ERROR_STOP=1 -d postgres -f scripts/reset-local-app-schema.sql",
    "db:studio": "node scripts/run-with-env-local.mjs prisma studio",
    "seed:check": "node scripts/seed-data.mjs --check",
    "seed:populate": "node scripts/seed-data.mjs"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.74.0",
    "@google/generative-ai": "^0.24.1",
    "@prisma/client": "^6.19.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "framer-motion": "^12.34.0",
    "lucide-react": "^0.563.0",
    "next": "16.1.6",
    "next-auth": "^4.24.13",
    "openai": "^6.22.0",
    "radix-ui": "^1.4.3",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "tailwind-merge": "^3.4.0"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "16.1.6",
    "prisma": "^6.19.2",
    "shadcn": "^3.8.4",
    "tailwindcss": "^4",
    "vitest": "^3.2.4",
    "tw-animate-css": "^1.4.0",
    "typescript": "^5"
  }
}
