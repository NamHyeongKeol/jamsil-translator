{
  "name": "mingle-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "build:mobile": "pnpm run build && node scripts/prepare-capacitor-web.mjs",
    "start": "next start",
    "lint": "eslint",
    "cap:add:ios": "pnpm exec cap add ios",
    "cap:add:android": "pnpm exec cap add android",
    "cap:sync": "pnpm run build:mobile && pnpm exec cap sync",
    "cap:open:ios": "pnpm exec cap open ios",
    "cap:open:android": "pnpm exec cap open android",
    "cap:verify": "node scripts/verify-capacitor.mjs",
    "seed:check": "node scripts/seed-data.mjs --check",
    "seed:populate": "node scripts/seed-data.mjs",
    "crawl:instagram": "node scripts/crawl-instagram.mjs",
    "crawl:hellotalk": "node scripts/crawl-hellotalk.mjs"
  },
  "dependencies": {
    "@capacitor/core": "^8.1.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.563.0",
    "next": "16.1.6",
    "next-auth": "^4.24.13",
    "radix-ui": "^1.4.3",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "tailwind-merge": "^3.4.0"
  },
  "devDependencies": {
    "@capacitor/android": "^8.1.0",
    "@capacitor/cli": "^8.1.0",
    "@capacitor/ios": "^8.1.0",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "16.1.6",
    "shadcn": "^3.8.4",
    "tailwindcss": "^4",
    "tw-animate-css": "^1.4.0",
    "typescript": "^5"
  }
}
