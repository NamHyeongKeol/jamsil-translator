# Core auth
AUTH_SECRET=

# Google OAuth
AUTH_GOOGLE_ID=
AUTH_GOOGLE_SECRET=

# Apple OAuth
# client id (Service ID for web Sign in with Apple)
AUTH_APPLE_ID=

# Option A: provide pre-generated client secret JWT directly
AUTH_APPLE_SECRET=

# Option B: provide key materials and let server generate AUTH_APPLE_SECRET
AUTH_APPLE_TEAM_ID=
AUTH_APPLE_KEY_ID=
# Paste p8 content with escaped newlines, for example:
# AUTH_APPLE_PRIVATE_KEY="-----BEGIN PRIVATE KEY-----\n...\n-----END PRIVATE KEY-----"
AUTH_APPLE_PRIVATE_KEY=
# optional (max 15552000 = 180 days), default: 15465600 (179 days)
AUTH_APPLE_CLIENT_SECRET_TTL_SECONDS=15465600

# Apple native auth (iOS NativeAuthModule - Apple ID token audience 검증)
# AUTH_APPLE_NATIVE_AUDIENCES: 쉼표 구분 허용 audience 목록. 미설정 시 native Apple 로그인 500 에러 발생.
# 일반적으로 번들 ID (예: com.minglelabs.mingle)를 설정.
AUTH_APPLE_NATIVE_AUDIENCES=
# AUTH_APPLE_NATIVE_ID / AUTH_APPLE_BUNDLE_ID 중 하나로도 설정 가능 (AUTH_APPLE_NATIVE_AUDIENCES 없을 때 폴백)
AUTH_APPLE_NATIVE_ID=
AUTH_APPLE_BUNDLE_ID=

# NextAuth / app URLs
NEXTAUTH_URL=http://localhost:3000
NEXT_PUBLIC_SITE_URL=http://localhost:3000

# RN runtime
NEXT_PUBLIC_WS_URL=ws://127.0.0.1:3001
NEXT_PUBLIC_API_NAMESPACE=ios/v1.0.0
